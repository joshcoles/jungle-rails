<%= form_tag '/login' do %>
  <div class="jumbotron">
    <h1>Login</h1>
      <div class="row">
        <div class="col-xs-4">
          Email: <%= text_field_tag :email, nil, class: 'form-control' %>
        </div>
      </div>
      <div class="row">
        <div class="col-xs-4">
          Password: <%= password_field_tag :password, nil, class: 'form-control' %>
        </div>
      </div>
      <br>
      <%= submit_tag "Submit", class: "btn-primary" %>
  </div>
<% end %>










